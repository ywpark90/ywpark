<%= title_img %>
<div id="content_inside_wrapper" class="round">
	<div id="content_inside">
		<ul id="resume_accordion">
			<li>
				<a id="resume_accordion_sel">
					<%= image_tag("/images/resume/resume_bar_education.png",
						:mouseover => "/images/resume/resume_bar_education_hover.png") %>
					<table class="resume_content">
						<tr>
							<th>Education</th>
						</tr>
						<tr>
							<td class="fit"><b>Carnegie Mellon University</b> Pittsburgh, PA</td>
						</tr>
						<tr>
							<td class="fit">M.S. in Electrical and Computer Engineering</td>
							<td>December 2014</td>
						</tr>
						<tr>
							<td class="fit">B.S. in Electrical and Computer Engineering</td>
							<td>December 2013</td>
						</tr>
						<tr>
							<td class="fit">Minor in Computer Science</td>
						</tr>
						<tr>
							<td class="fit indent">Overall GPA: 3.92/4.00</td>
						</tr>
					</table>
				</a>
			</li>

			<li>
				<a>
					<%= image_tag("/images/resume/resume_bar_skills.png",
						:mouseover => "/images/resume/resume_bar_skills_hover.png") %>
				</a>
			</li>

			<li>
				<a>
					<%= image_tag("/images/resume/resume_bar_work.png",
						:mouseover => "/images/resume/resume_bar_work_hover.png") %>
				</a>
			</li>

			<li>
				<a>
					<%= image_tag("/images/resume/resume_bar_projects.png",
						:mouseover => "/images/resume/resume_bar_projects_hover.png") %>
				</a>
			</li>

			<li>
				<a>
					<%= image_tag("/images/resume/resume_bar_awards.png",
						:mouseover => "/images/resume/resume_bar_awards_hover.png") %>
				</a>
			</li>
		</ul>
	</div>
</div>
		
<%= image_tag("content/sectionline.png", :alt => "Section Line",
					 		:id => "content_sectionline") %>
<div id="content_sectiontext">
	<%= image_tag("resume/sectiontext.png", :alt => "Section Text",
						 		:class => "icon") %>
	<%= image_tag("content/pdf_logo.png", :alt => "PDF Logo",
						 		:class => "icon") %>
	<%= image_tag("content/word_logo.png", :alt => "WORD Logo",
						 		:class => "icon") %>
	<%= image_tag("content/txt_logo.png", :alt => "TXT Logo",
						 		:class => "icon") %>
</div>
